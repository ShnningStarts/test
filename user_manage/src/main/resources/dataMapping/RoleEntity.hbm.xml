<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.smart.entity.Role" table="role" schema="user_base">
        <id name="id">
            <column name="id" sql-type="char(32)" length="32"/>
        </id>
        <property name="normalUser">
            <column name="normal_user" sql-type="int(2)"/>
        </property>
        <property name="admin">
            <column name="admin" sql-type="int(2)"/>
        </property>
        <property name="business">
            <column name="business" sql-type="int(2)"/>
        </property>
    </class>
</hibernate-mapping>